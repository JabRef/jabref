<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-US">
  <info>
    <title>The Geological Society of America</title>
    <title-short>GSA</title-short>
    <id>http://www.zotero.org/styles/the-geological-society-of-america</id>
    <link href="http://www.zotero.org/styles/the-geological-society-of-america" rel="self"/>
    <link href="http://www.geosociety.org/gsa/pubs/geology/home.aspx#overview" rel="documentation"/>
    <link href="http://www.geosociety.org/documents/gsa/pubs/GSA_RefGuide_Examples.pdf" rel="documentation"/>
    <author>
      <name>Rintze Zelle</name>
      <uri>http://twitter.com/rintzezelle</uri>
    </author>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <category citation-format="author-date"/>
    <category field="geology"/>
    <updated>2018-09-26T13:06:59+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor-translator">
    <names variable="editor translator" prefix="(" suffix=")" delimiter=", ">
      <name and="symbol" initialize-with=". " delimiter=", "/>
      <label form="short" prefix=", " text-case="capitalize-first"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name name-as-sort-order="all" and="text" sort-separator=", " initialize-with="." delimiter=", " delimiter-precedes-last="always"/>
      <label form="short" prefix=" (" suffix=")" text-case="capitalize-first"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name form="short" and="text" delimiter=", " initialize-with="."/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
            <text variable="title" form="short" font-style="italic"/>
          </if>
          <else>
            <text variable="title" form="short" quotes="true"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
      <group delimiter=" ">
        <text variable="collection-title"/>
        <text variable="collection-number"/>
      </group>
      <text variable="volume" prefix="v. "/>
      <choose>
        <if variable="page">
          <text variable="page" prefix="p. "/>
        </if>
        <else>
          <text variable="number-of-pages" suffix=" p."/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text variable="DOI" prefix="doi:"/>
      </if>
      <else-if variable="URL">
        <group>
          <text variable="URL" prefix=" "/>
          <date variable="accessed" prefix=" (accessed " suffix=")">
            <date-part name="month" suffix=" "/>
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
    </choose>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" collapse="year">
    <sort>
      <key variable="issued"/>
      <key macro="author-short"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=", ">
        <text macro="author-short"/>
        <date variable="issued">
          <date-part name="year"/>
        </date>
        <group>
          <label variable="locator" form="short"/>
          <text variable="locator" prefix=" "/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="11" et-al-use-first="1" hanging-indent="true">
    <sort>
      <key macro="author-short"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <group delimiter=", ">
        <text macro="author"/>
        <date variable="issued" suffix=",">
          <date-part name="year"/>
        </date>
      </group>
      <choose>
        <if type="report">
          <text variable="title" prefix=" " suffix=":"/>
          <group prefix=" " delimiter=" ">
            <text variable="publisher"/>
            <text variable="collection-title"/>
            <text variable="genre"/>
            <text variable="number"/>
          </group>
          <text variable="page" prefix=", " suffix=" p."/>
        </if>
        <else-if type="bill book graphic legal_case legislation motion_picture song" match="any">
          <group suffix=":">
            <text variable="title" prefix=" "/>
            <text macro="editor-translator" prefix=" "/>
          </group>
          <text prefix=" " macro="publisher"/>
        </else-if>
        <else-if type="chapter paper-conference" match="any">
          <text variable="title" prefix=" "/>
          <group prefix=", ">
            <text term="in" font-style="italic"/>
            <names variable="editor translator" prefix=" " suffix="," delimiter=", ">
              <name and="text" sort-separator=", " initialize-with="." name-as-sort-order="all"/>
              <label form="short" prefix=" "/>
            </names>
            <text variable="container-title" prefix=" " suffix=","/>
            <text macro="publisher" prefix=" "/>
          </group>
        </else-if>
        <else-if type="map">
          <text variable="title" prefix=" " suffix=":"/>
          <group delimiter=" " prefix=" ">
            <text variable="publisher"/>
            <text variable="collection-title"/>
            <text variable="genre"/>
          </group>
          <text variable="note" prefix=", scale "/>
        </else-if>
        <else-if type="thesis">
          <text variable="title" prefix=" "/>
          <text variable="genre" prefix=" [" suffix="]"/>
          <group prefix=": " delimiter=", ">
            <text variable="publisher"/>
            <text variable="number-of-pages" suffix=" p."/>
          </group>
        </else-if>
        <else>
          <group suffix=":">
            <text variable="title" prefix=" "/>
            <text macro="editor-translator" prefix=" "/>
          </group>
          <group prefix=" ">
            <text variable="container-title" suffix=","/>
            <group prefix=" ">
              <text variable="volume" prefix="v. "/>
            </group>
            <group prefix=", " delimiter=", ">
              <text variable="page" prefix="p. "/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="access" prefix=", "/>
    </layout>
  </bibliography>
</style>
