  <locale xml:lang="sl">
    <terms>
      <term name="et-al">idr.</term>
      <term name="editortranslator" form="short">
        <single>ed. &amp; trans.</single>
        <multiple>eds. &amp; trans.</multiple>
      </term>
      <term name="editor" form="short">ur.</term>
      <term name="interviewer" form="short">intervjuvanec</term>
      <term name="circa" form="short">ca.</term>
      <term name="collection-editor" form="short">ur.</term>
      <term name="edition" form="long">izdaja</term>
    </terms>
  </locale>
  <macro name="container">
    <choose>
      <if type="chapter paper-conference entry-encyclopedia" match="any">
        <text term="in" text-case="uppercase" prefix=". " suffix=": "/>
        <names variable="editor translator" delimiter=", " suffix=": ">
          <name name-as-sort-order="all" sort-separator=", " initialize-with="." delimiter=", " delimiter-precedes-last="always"/>
          <label form="short" text-case="lowercase" prefix=" (" suffix=")"/>
        </names>
        <group delimiter=", ">
          <text variable="container-title" text-case="title" font-style="italic"/>
          <text variable="collection-title" text-case="title"/>
        </group>
      </if>
      <else-if type="bill book graphic legal_case legislation motion_picture report song" match="any">
        <group prefix=", " delimiter=", ">
          <text variable="container-title"/>
          <text variable="collection-title"/>
        </group>
      </else-if>
      <else-if type="webpage" match="any">
        <group delimiter=", ">
          <text variable="container-title"/>
          <text variable="collection-title"/>
        </group>
      </else-if>
      <else-if type="article-magazine" match="any"/>
      <else-if type="article" match="any">
        <choose>
          <if match="none" variable="author">
            <group>
              <text variable="title"/>
              <text variable="publisher-place" prefix=". "/>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset" match="any">
        <text variable="title" font-style="italic"/>
      </else-if>
      <else>
        <group prefix=". " delimiter=", ">
          <text variable="container-title" font-style="italic" suffix=","/>
          <text variable="collection-title"/>
        </group>
      </else>
    </choose>
    <text macro="locators"/>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter-precedes-last="always" initialize-with="." name-as-sort-order="all"/>
      <label form="short" text-case="lowercase" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name form="short" and="text" delimiter=", " initialize-with=". "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
            <text variable="title" form="short" font-style="italic"/>
          </if>
          <else-if type="webpage" match="any">
            <text variable="container-title"/>
          </else-if>
          <else-if type="dataset" match="any">
            <text variable="publisher"/>
          </else-if>
          <else>
            <text variable="original-publisher" form="short" quotes="false"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text variable="DOI" prefix="https://doi.org/"/>
      </if>
      <else-if type="webpage post-weblog" match="any">
        <group delimiter=" ">
          <text value="Dostopno na: "/>
          <text variable="URL"/>
          <group prefix="(" suffix=").">
            <text value="sneto" suffix=" "/>
            <date variable="accessed">
              <date-part name="day" suffix=". "/>
              <date-part name="month" form="numeric" suffix=". "/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="report thesis" match="any">
        <text variable="title" font-style="italic"/>
        <group delimiter=" " prefix=". ">
          <text variable="genre" text-case="capitalize-first"/>
          <text variable="number" prefix="No. "/>
        </group>
      </if>
      <else-if type="bill book graphic legal_case motion_picture report song speech" match="any">
        <text variable="title" font-style="italic"/>
        <text macro="edition" prefix=", "/>
      </else-if>
      <else-if type="webpage">
        <text variable="container-title"/>
      </else-if>
      <else-if type="legislation" match="any">
        <text variable="title"/>
      </else-if>
      <else-if type="article" match="any">
        <text variable="publisher"/>
      </else-if>
      <else-if type="dataset" match="any">
        <text variable="publisher"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="event">
    <choose>
      <if variable="event">
        <text term="presented at" text-case="capitalize-first" suffix=" "/>
        <text variable="event"/>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <group delimiter=" ">
      <choose>
        <if is-numeric="edition">
          <number variable="edition" form="ordinal"/>
        </if>
        <else>
          <text variable="edition" suffix="."/>
        </else>
      </choose>
      <text value="izdaja"/>
    </group>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal article-magazine article-newspaper" match="any">
        <group delimiter=", " prefix=" ">
          <group>
            <text variable="volume"/>
            <text variable="issue" prefix="(" suffix=")"/>
          </group>
          <group>
            <text value="str. "/>
            <text variable="page"/>
          </group>
        </group>
      </if>
      <else-if type="bill book graphic legal_case legislation motion_picture report song thesis" match="any">
        <group delimiter=", " prefix=". ">
          <text macro="event"/>
          <text macro="publisher"/>
        </group>
      </else-if>
      <else-if type="chapter paper-conference entry-encyclopedia" match="any">
        <group delimiter=". " prefix=", ">
          <group>
            <label text-case="lowercase" suffix=" " variable="page" form="short"/>
            <text variable="page"/>
          </group>
          <text macro="event"/>
          <text macro="publisher"/>
        </group>
      </else-if>
      <else-if type="patent">
        <text variable="number" prefix=". "/>
      </else-if>
      <else-if type="dataset" match="any">
        <group prefix=", ">
          <text macro="publisher"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <citation delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="3" et-al-use-first="1" disambiguate-add-givenname="true" disambiguate-add-year-suffix="true" collapse="year">
    <sort>
      <key macro="issued"/>
      <key macro="author"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=", ">
        <text macro="author-short"/>
        <text macro="issued"/>
        <group delimiter=" ">
          <label variable="locator" form="short"/>
          <text variable="locator"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="6" entry-spacing="2" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key macro="issued" sort="descending"/>
    </sort>
    <layout>
      <group suffix=".">
        <text macro="author"/>
        <group>
          <choose>
            <if type="legislation" match="none">
              <group prefix=" (" suffix=")">
                <text macro="issued"/>
              </group>
            </if>
            <else>
              <text variable="container-title" prefix=". "/>
              <text variable="publisher-place"/>
            </else>
          </choose>
        </group>
        <group prefix=": ">
          <choose>
            <if type="legislation" match="none">
              <text macro="title"/>
              <text macro="container"/>
            </if>
          </choose>
        </group>
      </group>
      <text macro="access" prefix=". "/>
    </layout>
  </bibliography>
</style>
