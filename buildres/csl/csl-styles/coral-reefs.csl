  <macro name="author">
    <names variable="author" suffix=" ">
      <name delimiter=", " name-as-sort-order="all" sort-separator=" " form="long" initialize-with=""/>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="In: " suffix=" (eds) ">
      <name delimiter=", " name-as-sort-order="all" sort-separator=" " form="long" initialize-with="."/>
    </names>
  </macro>
  <macro name="year">
    <date variable="issued" prefix="(" suffix=") ">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="title">
    <text variable="title" suffix=". "/>
  </macro>
  <citation disambiguate-add-year-suffix="true" collapse="year" et-al-min="3" et-al-use-first="1">
    <sort>
      <key macro="year"/>
      <key variable="issued" sort="ascending"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=" ">
        <names variable="author">
          <name and="text" delimiter=" " name-as-sort-order="all" sort-separator=", " form="short"/>
        </names>
        <date variable="issued">
          <date-part name="year" form="long"/>
        </date>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key macro="year" sort="ascending"/>
    </sort>
    <layout>
      <text macro="author"/>
      <text macro="year"/>
      <text macro="title"/>
      <choose>
        <if type="thesis">
          <group delimiter=", ">
            <text variable="genre"/>
            <text variable="publisher"/>
            <text variable="page" prefix="p "/>
          </group>
        </if>
        <else-if type="book">
          <text variable="publisher" suffix=", "/>
          <text variable="publisher-place" suffix=" "/>
        </else-if>
        <else-if type="paper-conference">
          <text variable="event" prefix=" " strip-periods="true" suffix=" "/>
          <text variable="volume" suffix=":"/>
          <text variable="page"/>
        </else-if>
        <else-if type="chapter">
          <text macro="editor"/>
          <text variable="container-title" prefix=" " suffix=". "/>
          <text variable="publisher" suffix=", "/>
          <text variable="publisher-place" suffix=", "/>
          <text variable="page" prefix="pp " suffix=" "/>
        </else-if>
        <else-if type="webpage">
          <text variable="URL" suffix=" "/>
        </else-if>
        <else>
          <group suffix=" ">
            <text variable="container-title" form="short" strip-periods="true" suffix=" "/>
            <text variable="volume" suffix=":"/>
            <text variable="page"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
