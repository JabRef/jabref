    <author>
      <name>Patrick O'Brien</name>
      <email>obrienpat86@gmail.com</email>
    </author>
    <category citation-format="numeric"/>
    <category field="biology"/>
    <category field="medicine"/>
    <issn>1538-6341</issn>
    <eissn>1931-2393</eissn>
    <updated>2017-06-25T13:22:54+00:00</updated>
