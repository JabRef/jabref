  </info>
  <macro name="author">
    <names variable="author">
      <name et-al-min="4" et-al-use-first="1" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="long" prefix=", "/>
      <substitute>
        <names variable="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" suffix=". ">
      <name initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label strip-periods="true" prefix=", "/>
    </names>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <text variable="title"/>
    </group>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout vertical-align="sup" delimiter=",">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="4" et-al-use-first="1">
    <sort>
      <key macro="author"/>
    </sort>
    <layout suffix=".">
      <text variable="citation-number" font-weight="bold" suffix=". "/>
      <text macro="author" suffix=", "/>
      <text macro="title" suffix=". "/>
      <choose>
        <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
          <group delimiter="; ">
            <group delimiter=" " prefix=" ">
              <text macro="editor"/>
              <text macro="publisher"/>
            </group>
            <date date-parts="year" form="text" variable="issued"/>
          </group>
        </if>
        <else-if type="chapter paper-conference" match="any">
          <group prefix=" " suffix=". ">
            <text term="in" text-case="capitalize-first" suffix=": "/>
            <text macro="editor"/>
            <text variable="container-title"/>
          </group>
          <group delimiter=": ">
            <group delimiter="; ">
              <text macro="publisher" prefix=" "/>
              <date date-parts="year" form="text" variable="issued"/>
            </group>
            <text variable="page"/>
          </group>
        </else-if>
        <else-if type="article-journal" match="any">
          <choose>
            <if variable="issue" match="any">
              <group delimiter=":">
                <group delimiter=", ">
                  <text variable="container-title" font-style="italic"/>
                  <date variable="issued">
                    <date-part name="year"/>
                  </date>
                  <group>
                    <text variable="volume"/>
                    <text variable="issue" prefix="(" suffix=")"/>
                  </group>
                </group>
                <text variable="page"/>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <date variable="issued">
                  <date-part name="year"/>
                  <date-part name="month" prefix=" "/>
                  <date-part name="day" prefix=" "/>
                </date>
                <text value="Epub"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if type="post-weblog webpage" match="any">
          <text variable="container-title" suffix=". "/>
          <date form="text" variable="issued" suffix=". "/>
          <group delimiter=". ">
            <text variable="URL"/>
            <group delimiter=" ">
              <text term="accessed" text-case="capitalize-first"/>
              <date form="text" variable="accessed"/>
            </group>
          </group>
        </else-if>
        <else>
          <text variable="container-title" form="short"/>
          <group suffix=";">
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </group>
          <group>
            <group>
              <text variable="volume"/>
              <text variable="issue" prefix="(" suffix=")"/>
            </group>
            <text variable="page" prefix=":"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
