<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-GB">
  <info>
    <title>WHO Regional Office for Europe (numeric)</title>
    <id>http://www.zotero.org/styles/who-europe-numeric</id>
    <link href="http://www.zotero.org/styles/who-europe-numeric" rel="self"/>
    <link href="https://github.com/citation-style-language/styles/pull/170" rel="documentation"/>
    <author>
      <name>Ivo Rakovac</name>
      <email>irk@euro.who.int</email>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <summary>The WHO Regional Office for Europe numeric style</summary>
    <updated>2013-02-28T13:00:00+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor">
      <name name-as-sort-order="all" sort-separator=" " initialize-with="" delimiter=", " delimiter-precedes-last="always"/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <!--macro name="anon">
<text term="" form="short" text-case="capitalize-first" strip-periods="true"/>
</macro-->
  <macro name="author">
    <choose>
      <if variable="call-number" match="none">
        <group suffix=".">
          <names variable="author">
            <name name-as-sort-order="all" sort-separator=" " initialize-with="" delimiter=", " delimiter-precedes-last="always"/>
            <label form="short" strip-periods="true"/>
            <substitute>
              <names variable="editor"/>
              <!--text macro="anon"/-->
            </substitute>
          </names>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="URL">
        <group delimiter=", ">
          <text variable="URL"/>
          <group>
            <text value="accessed "/>
            <date variable="accessed">
              <date-part name="day" suffix=" "/>
              <date-part name="month" suffix=" "/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="accessSeries">
    <choose>
      <if variable="URL collection-title collection-number" match="any">
        <text value="("/>
        <group delimiter="; ">
          <text macro="series"/>
          <text macro="access"/>
        </group>
        <text value=")"/>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="book" variable="archive_location" match="all">
        <text variable="title"/>
        <text variable="archive_location" prefix=" [" suffix="]"/>
      </if>
      <else-if type="thesis" match="all">
        <text variable="title" font-style="italic"/>
        <text variable="genre" prefix=" [" suffix="]"/>
      </else-if>
      <else-if type="bill book graphic legal_case report" match="any">
        <text variable="title" font-style="italic"/>
      </else-if>
      <else-if type="webpage" match="all">
        <text variable="title" suffix=" [web site]"/>
      </else-if>
      <else-if type="broadcast" match="all">
        <text variable="title"/>
        <text variable="medium" prefix=" [" suffix="]"/>
      </else-if>
      <else-if type="song motion_picture" match="any">
        <text variable="title" font-style="italic"/>
        <text variable="medium" prefix=" [" suffix="]"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher-place"/>
      <text variable="publisher"/>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix="."/>
      </else>
    </choose>
  </macro>
  <macro name="series">
    <choose>
      <if variable="collection-title collection-number" match="any">
        <group>
          <text variable="collection-title"/>
          <text variable="collection-number" prefix=", No. "/>
        </group>
      </if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="," font-style="italic">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography hanging-indent="false" et-al-min="4" et-al-use-first="1">
    <layout>
      <text variable="citation-number" suffix=". "/>
      <group suffix=".">
        <text macro="author" suffix=" "/>
        <choose>
          <if type="bill book graphic legal_case motion_picture song report thesis" match="any">
            <group suffix=".">
              <text macro="title"/>
              <text macro="edition" prefix=", "/>
              <text macro="editor" prefix=" (" suffix=")"/>
            </group>
            <text prefix=" " macro="publisher"/>
            <group prefix=", ">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <text variable="page" prefix=":"/>
            </group>
          </if>
          <else-if type="chapter" match="any">
            <text macro="title" suffix="."/>
            <group prefix=" ">
              <text term="in" text-case="capitalize-first" suffix=": "/>
              <text macro="editor"/>
              <text variable="container-title" font-style="italic" prefix=" " suffix="."/>
              <text variable="volume" prefix="Vol" suffix="."/>
              <text macro="edition" prefix=","/>
              <text variable="collection-title" suffix="."/>
              <group suffix="." prefix=" ">
                <text macro="publisher"/>
                <group suffix="." prefix=", ">
                  <date variable="issued">
                    <date-part name="year"/>
                  </date>
                  <text variable="page" prefix=":"/>
                </group>
              </group>
            </group>
          </else-if>
          <else-if type="paper-conference" match="any">
            <text macro="title" suffix="."/>
            <group prefix=" ">
              <choose>
                <if variable="editor">
                  <text term="in" text-case="capitalize-first" suffix=": " prefix=" "/>
                  <text macro="editor" suffix=" "/>
                  <text variable="container-title" font-style="italic" suffix="."/>
                  <text variable="volume" prefix="Vol" suffix="."/>
                  <text macro="edition" prefix=" "/>
                  <text variable="collection-title" suffix="."/>
                  <group>
                    <text macro="publisher" prefix=" "/>
                    <group suffix="." prefix=", ">
                      <date variable="issued">
                        <date-part name="year"/>
                      </date>
                      <text variable="page" prefix=":"/>
                    </group>
                  </group>
                </if>
                <else>
                  <!-- no editor given -->
                  <text variable="event" font-style="italic" suffix="."/>
                  <group>
                    <text macro="publisher" prefix=" "/>
                    <group prefix=", ">
                      <date variable="issued">
                        <date-part name="year"/>
                      </date>
                    </group>
                  </group>
                </else>
              </choose>
            </group>
          </else-if>
          <else-if type="webpage" match="any">
            <text macro="title" suffix="."/>
            <text variable="note" prefix=" "/>
            <date variable="issued" prefix=", ">
              <date-part name="year"/>
            </date>
          </else-if>
          <else-if type="article-newspaper broadcast" match="any">
            <group suffix=".">
              <text macro="title" suffix=". "/>
              <group suffix=", " delimiter=" ">
                <text variable="container-title" font-style="italic"/>
                <text variable="publisher-place" prefix="(" suffix=")"/>
              </group>
              <group>
                <date variable="issued">
                  <date-part name="day" suffix=" "/>
                  <date-part name="month" suffix=" "/>
                  <date-part name="year"/>
                </date>
              </group>
              <text variable="section" prefix=", Section "/>
              <text variable="page" prefix=":"/>
            </group>
          </else-if>
          <else>
            <group suffix=".">
              <text macro="title"/>
              <text macro="editor"/>
            </group>
            <group prefix=" " suffix=".">
              <text variable="container-title" font-style="italic"/>
              <group prefix=", ">
                <date variable="issued">
                  <date-part name="year"/>
                </date>
                <group prefix=", ">
                  <text variable="volume"/>
                  <text variable="issue" prefix="(" suffix=")"/>
                </group>
              </group>
              <text variable="page" prefix=":"/>
            </group>
          </else>
        </choose>
        <text prefix=" " macro="accessSeries"/>
      </group>
    </layout>
  </bibliography>
</style>
