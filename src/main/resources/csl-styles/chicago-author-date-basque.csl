      <!-- LONG ROLE FORMS -->
      <term name="author">
        <single/>
        <multiple/>
      </term>
      <term name="editor">
        <single>argitaratzailea</single>
        <multiple>argitaratzaileak</multiple>
      </term>
      <term name="editorial-director">
        <single>argitaratzailea</single>
        <multiple>argitaratzaileak</multiple>
      </term>
      <term name="translator">
