<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<DialogPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.jabref.gui.exporter.ExportCustomizationDialogView">
   <content>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <TableView prefHeight="150.0" prefWidth="500.0">
              <columns>
                <TableColumn prefWidth="50.0" text="%Export Name" />
                <TableColumn prefWidth="200.0" text="%Main Layout File" />
                <TableColumn prefWidth="30.0" text="%File extension" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </content>
   <ButtonType fx:id="addButton" text="%Add" />
   <ButtonType fx:id="modifyButton" text="%Modify" />
   <ButtonType fx:id="removeButton" text="%Remove" />
   <ButtonType fx:id="closeButton" fx:constant="OK" />
   <ButtonType fx:constant="CANCEL" />
</DialogPane>
