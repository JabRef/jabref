  issn            = {2165-9559},
  journal         = {2024 IEEE Global Engineering Education Conference (EDUCON)},
  keywords        = {Economics, Electronic learning, Distance learning, Employment, Collaboration, Propulsion, Engineering education, Education Transformation, Lifelong Learning, Digital Education, Global Competitiveness, Remote Work Skills},
}

@Article{Helden2023,
  author    = {Gitte Van Helden and Vivian Van Der Werf and Gillian N. Saunders-Smits and Marcus M. Specht},
