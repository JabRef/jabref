@InProceedings{Sockalingam2023,
  author          = {Nachamma Sockalingam and Kenneth Lo and Pey Kin Leong},
  title           = {SUTD campusX: An Institutional Digital Transformation Approach to Higher Education},
  year            = {2023},
  address         = {Monterrey, Mexico},
  pages           = {1--6},
  publisher       = {IEEE},
  abstract        = {The recent Covid-19 pandemic has created a strong momentum for educational institutions across the world to transform their digital education. While online learning has its benefits and is valuable for distance and adult learning, there are several known limitations in terms of overall student satisfaction, social connectivity, student well-being, cost of transformation, student and faculty preparedness, and challenges in converting certain learning activities such as hands-on experiments and labs to be online. Our university, Singapore University of Technology and Design, uses signature student-centric pedagogies characterized by an interdisciplinary curriculum, learning and teaching in teams, design-centric project work, and assessments. This includes both Pre-employment Training (PET) and Continuing Education and Training (CET). Although existing technologies such as Zoom and TEAMS can be adapted easily for self-directed online courses such as MOOCs, the question is "How can we possibly use technology for student-centric teaching which involves interdisciplinary curriculum, and hands-on collaborative projects?" This requires both bottom-up and top-down changes in the university-level strategies for teaching and learning. Faculty members need to innovate teaching and learning, and university leadership, together with support services must strategize and present a sharp vision, clear directions, and supportive resources for moving this forward. To this end, our university has launched a campus-wide program to spur technology innovation for the ever-changing norm of teaching and learning. In this reflection paper, we will share our campusX initiative which focuses on cyber-physical learning to innovating digital learning.},
  date            = {23-27 Oct. 2023},
  doi             = {10.1109/WEEF-GEDC59520.2023.10343873},
  eventdate       = {23-27 Oct. 2023},
  eventtitleaddon = {Monterrey, Mexico},
  file            = {:https\://ieeexplore.ieee.org/stamp/stamp.jsp?tp=&arnumber=10343873:PDF},
  isbn            = {979-8-3503-1603-2},
  issn            = {2837-5017},
  journal         = {2023 World Engineering Education Forum - Global Engineering Deans Council (WEEF-GEDC)},
  keywords        = {Training, Technological innovation, Leadership, Costs, Pandemics, Digital transformation, Scalability, cyber-physical learning, digital transformation, higher education, institutional approach, student-centered learning, SUTD campusX},
}

@InProceedings{Chituc2022,
  author          = {Claudia-Melania Chituc},
  title           = {An Analysis of Technical Challenges for Education 4.0 and Digital Education Ecosystems},
  year            = {2022},
  address         = {Berlin, Germany},
  pages           = {1--5},
  publisher       = {IEEE},
  abstract        = {Industry 4.0 and its required digital transformation remodel the way industrial business processes are executed, and require highly qualified workforce. The term Education 4.0 emerged to tackle the preparation of future workforce for Industry 4.0. Although highly relevant for the success of Industry 4.0, this topic is in an early stage, and research and development work expends without a clear vision how the Industry 4.0 necessities of skills and competences of the workforce will be addressed, and how the emerging Digital Education Ecosystems brace the goals of Education 4.0. The aim of this work-in-progress article is to provide insights about the current status and trends of technical challenges for Education 4.0 and Digital Education Ecosystems. To do so, an analysis of relevant articles was executed following PRISMA guidelines. 16 works were selected for this analysis. Five technical challenges are identified and analyzed: (1) information and communication technology infrastructure, (2) interoperability, (3) development of artificial intelligence algorithms to aid education, (4) implementation, (5) security, data protection and privacy. A framework is proposed to synthesize the results of the analysis performed, reflecting the technical challenges identified and solution approaches.},
  date            = {11-12 Aug. 2022},
  doi             = {10.1109/GeCon55699.2022.9942758},
  eventdate       = {11-12 Aug. 2022},
  eventtitleaddon = {Berlin, Germany},
  file            = {:https\://ieeexplore.ieee.org/stamp/stamp.jsp?tp=&arnumber=9942758:PDF},
  isbn            = {978-1-6654-5171-0},
  journal         = {2022 IEEE German Education Conference (GeCon)},
  keywords        = {Industries, Education, Ecosystems, Market research, Fourth Industrial Revolution, Information and communication technology, Security, Industry 4.0, Education 4.0, Technical Challenges, Digital Education Ecosystem},
}

