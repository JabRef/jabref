@InProceedings{Chen2020,
  author          = {Kai-Sheng Chen and Han-Teng Liao},
  title           = {Mapping the Field of Educational Assessment and Professional Accreditation in Higher Education: Bibliographic Coupling and Co-Citation Analyses for Green and Digital Transformation},
  year            = {2020},
  address         = {Guangzhou, China},
  pages           = {281--284},
  publisher       = {IEEE},
  abstract        = {Evaluating effectiveness and impacts of higher education has been the central sites of observations for the field of educational assessment and professional accreditation, with various institutions providing accreditation services such as IEET, HEEACT, AACSB, ABET, etc. Given the advancement of digital technologies and digital economy applications, it is important to explore how such higher education evaluation can benefit from, and also account in, such digital advancement. In this paper, we conduct web crawling written in Python to systematically scan the Web of Science to retrieve the related literature. Based on 646 bibliographic data collected from the database, the findings show that the main contribution has been made by mostly the three main disciplines: science education, educational research, and engineering, the top journals such as International Journal of Engineering Education, the top keywords such as accreditation, assessment, quality assurance, outcomes, etc. The findings also reveal how such concerns have been discussed in relation to topics such as learning analytics, big data, sustainability, impacts, etc., that can be related to green and digital transformation. In conclusion, the overall mapping provides an initial roadmap for the practice of higher education assessment and accreditation to harness the potentials of digital technologies for green and digital transformation of higher education, including more inclusive notion of "impact" that includes sustainability and more human-centered notion of "outcomes" that focuses more on human development.},
  date            = {18-20 Dec. 2020},
  doi             = {10.1109/MSIEID52046.2020.00058},
  eventdate       = {18-20 Dec. 2020},
  eventtitleaddon = {Guangzhou, China},
  file            = {:https\://ieeexplore.ieee.org/stamp/stamp.jsp?tp=&arnumber=9382526:PDF},
  isbn            = {978-1-6654-4705-8},
  journal         = {2020 Management Science Informatization and Economic Innovation Development Conference (MSIEID)},
  keywords        = {Quality assurance, Databases, Digital transformation, Education, Big Data, Accreditation, Sustainable development, sustainability education, university impact, impact assessment, qualtiy assurance, sustainable development},
}

